{
  "name": "lcs-cloud-storage",
  "version": "0.0.1-alpha.1",
  "description": "Local cloud storage with authentication",
  "main": "bin/index.ts",
  "bin": {
    "lcs": "bin/index.ts"
  },
  "repository": "https://github.com/stbestichhh/cloud-storage.git",
  "author": "Kiril Yakymchuk <stbestich@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node bin/index.ts",
    "start:dev": "dotenv -e .env.development.local -- cross-env NODE_ENV=development nodemon bin/index.ts",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:cov": "dotenv -e .env.test -- jest --coverage",
    "test:e2e": "dotenv -e .env.test -- jest --config test/e2e/jest-e2e.ts",
    "test:e2e:watch": "dotenv -e .env.test -- jest --config test/e2e/jest-e2e.ts --watch"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "node-json-db": "^2.3.0",
    "uuid": "^9.0.1"
  }
