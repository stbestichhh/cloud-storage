{
  "name": "lcs-cloud-storage",
  "version": "1.0.0",
  "description": "Local cloud storage with authentication",
  "main": "dist/index.js",
  "bin": {
    "lcs": "dist/index.js"
  },
  "keywords": [
    "Cli",
    "Cloud storage",
    "Express.js"
  ],
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "repository": "https://github.com/stbestichhh/cloud-storage.git",
  "author": "Kiril Yakymchuk <stbestich@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "start": "ts-node bin/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "dotenv -e .env.development.local -- cross-env NODE_ENV=development nodemon bin/index.ts",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:cov": "dotenv -e .env.test -- jest --coverage",
    "test:e2e": "dotenv -e .env.test -- jest --config test/e2e/jest-e2e.ts",
    "test:e2e:watch": "dotenv -e .env.test -- jest --config test/e2e/jest-e2e.ts --watch"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.3",
    "@types/node-cron": "^3.0.11",
    "@types/pem": "^1.14.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@stlib/utils": "^0.0.9",
    "@types/express": "^4.17.21",
    "argon2": "^0.40.1",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pem": "^1.14.8",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  }
}
